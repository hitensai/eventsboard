<header>
	<h1>Users</h1>
</header>
<table class="table table-striped table-responsive" >
	
	<thead>
		<tr>
			<th>User Id</th>
			<th>Avatar</th>
			<th>Username</th>
			<th>Email</th>
			<th>Joined</th>
			<th>Events</th>

		</tr>
	</thead>
	<tbody>
		<%  @users.each do |user|%>
		<tr>
			<td><%= user.id %></td>
			<td>"Avatar"</td>
			<td><%= user.username %></td>
			<td><%= user.email %></td>
			<td><%= time_format(user.created_at) %></td>
            <td><%=  link_to user.organizeed_events.count, user_path(user) %></td>
		</tr>

		<% end %>
	</tbody>
</table>
<p><%= link_to "Back to Dashboard", admin_root_path %></p>