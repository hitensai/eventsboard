<h1>Edit event</h1>

<%= form_for(@event) do |f| %>
   <div>
 	<%= f.label :title %>
 	<%= f.text_field :title %>
   </div>
   <div>
 	<%= f.label :description %>
 	<%= f.text_area :description %>
   </div>
   <div>
 	<%= f.label :start_date %>
 	<%= f.datetime_select :start_date %>
   </div>
   <div>
 	<%= f.label :end_date %>
 	<%= f.datetime_select :end_date %>
   </div>
   <div>
 	<%= f.label :venue %>
 	<%= f.text_field :venue %>
   </div>
   <div>
 	<%= f.label :location %>
 	<%= f.text_field :location %>
   </div>

   <div>
   	<%= f.submit %>
   </div>
    
   


<% end %>

